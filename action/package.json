{
  "name": "fetch-github-starred-repos-action",
  "version": "1.0.0",
  "description": "A GitHub Action to fetch and store information about starred repositories",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --minify",
    "build:builder": "tsc build.ts --outDir . --target esnext --module commonjs --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports --strict --skipLibCheck --forceConsistentCasingInFileNames",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "github",
    "actions",
    "starred",
    "repositories"
  ],
  "author": "GitHub Stars Search",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@octokit/graphql": "^9.0.1",
    "@octokit/rest": "^22.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@vercel/ncc": "^0.38.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
